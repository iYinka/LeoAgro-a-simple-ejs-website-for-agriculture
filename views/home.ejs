<%- include("partials/header") -%>


 <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" data-interval="5000">
    <div class="carousel-inner">
      <div  class="carousel-item active">
        <img class="d-block " src="../images/eggs1.jpg" alt="eggs1">
      </div>
      <div class="carousel-item">
        <img class="d-block" src="../images/eggs2.jpg" alt="eggs2">
      </div>
      <div class="carousel-item">
        <img class="d-block" src="../images/farmers.jpg" alt="farmers">
      </div>
      <div class="carousel-item">
        <img class="d-block" src="../images/farmers3.jpg" alt="farmers3">
      </div>
    </div>
    <div class="slide-text">
        <h1>LeoAgro Farms</h1>
        <p>Giving you the <em>BEST</em> of farm produce.</p>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div> 
  

<div class="content">
    <p><%= mainContent %></p>
</div>
<%- include("partials/footer") -%>